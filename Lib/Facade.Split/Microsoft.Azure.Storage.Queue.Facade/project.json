{
  "title": "Microsoft.Azure.Storage.Queue",
  "version": "9.0.0-preview",
  "authors": [ "Microsoft Corporation" ],
  "description": "Azure Storage Queue SDK for NetCore",
  "dependencies": {
    "Microsoft.Azure.Storage.Common": "9.0.0.0-preview",
    "NETStandard.Library": "1.6.1"
  },

  "frameworks": {
    "netstandard1.0": {
      "imports": "dnxcore50"
    }
  },

  "buildOptions": {
    "allowUnsafe": false,
    "warningsAsErrors": true,
    "delaySign": true,
    "keyFile": "MSSharedLibKey.snk",
    "outputName": "Microsoft.Azure.Storage.Queue",
    "compile": [
      "**/*.cs",
      "Queue/*.cs"
    ],
    "define": [ "SIGN" ]
  }
}
