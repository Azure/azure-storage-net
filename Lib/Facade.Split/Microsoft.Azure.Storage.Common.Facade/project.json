{
  "title": "Microsoft.Azure.Storage.Common",
  "version": "9.4.0-preview",
  "authors": [ "Microsoft Corporation" ],
  "description": "Azure Storage Common SDK for NetCore",

  "dependencies": {
    "NETStandard.Library": "1.6.1"
  },

  "frameworks": {
    "netstandard1.0": {
      "imports": "dnxcore50"

    }

  },

  "buildOptions": {
    "allowUnsafe": false,
    "warningsAsErrors": true,
    "delaySign": true,
    "keyFile": "MSSharedLibKey.snk",
    "outputName": "Microsoft.Azure.Storage.Common",
    "compile": [
      "**/*.cs",
      "Common/*.cs"
    ],
    "define": [ "SIGN" ]
  }
}
