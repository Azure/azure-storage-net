{
  "title": "Microsoft.Azure.Storage.Blob",
  "version": "9.0.0-preview",
  "authors": [ "Microsoft Corporation" ],
  "description": "Azure Storage Blob SDK for NetCore",
  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "NETStandard.Library": "1.6.1",
        "Microsoft.Azure.Storage.Common": "9.0.0.0-preview"
      },
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
    "allowUnsafe": false,
    "warningsAsErrors": true,
    "delaySign": true,
    "keyFile": "MSSharedLibKey.snk",
    "outputName": "Microsoft.Azure.Storage.Blob",
    "compile": [
      "**/*.cs",
      "Blob/*.cs"
    ],
    "define": [ "SIGN" ]
  }
}
